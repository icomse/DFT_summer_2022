    TIMESTEP [fs] 0.5
    STEPS    8000
    TEMPERATURE 298
    &BAROSTAT
      TIMECON [fs] 100
      PRESSURE [bar] 1.0
    &END BAROSTAT
    &THERMOSTAT
      REGION GLOBAL
      TYPE CSVR
      &CSVR
        TIMECON [fs] 100.
      &END CSVR
    &END THERMOSTAT
  &END MD
                                                                                    
  &GEO_OPT
    TYPE MINIMIZATION
    MAX_DR    3.0E-03
    MAX_FORCE 3.0E-03
